<?xml version="1.0"?>
<config>
    <tabs>
        <maxvision translate="label">
            <label><![CDATA[<div style="background: url(data:image/png;base64,) no-repeat 0 0;" width: 155px; height:20px;">&nbsp;</div>]]>
            </label>
            <sort_order>1</sort_order>
        </maxvision>
    </tabs>
    <sections>
        <maxvision_customoptionsedit translate="label">
            <class>separator-top</class>
            <label>Custom Options Edit</label>
            <tab>maxvision</tab>
            <sort_order>1</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <customoptions_setting translate="label">
                    <label>Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label comment">
                            <label>Enabled</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enabled>
                        <orderhistory translate="label comment">
                            <label>Add Notice to Order History</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Adding notice to Order History when the Custom Options was modified.</comment>
                        </orderhistory>
                    </fields>
                </customoptions_setting>
            </groups>
        </maxvision_customoptionsedit>
    </sections>
</config>
